{
  "name": "vite-express-ssr-only",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@marko/express": "^1.0.0",
    "express": "^4.18.1",
    "marko": "^5.21.2"
  },
  "devDependencies": {
    "@marko/compiler": "^5.21.6",
    "@marko/vite": "^2.3.4",
    "vite": "^2.9.14"
  },
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production vite build --ssr src/index.js",
    "dev": "node --enable-source-maps index.js",
    "dev:inspect": "node --inspect --enable-source-maps index.js",
    "start": "NODE_ENV=production node --enable-source-maps index.js"
  }
}